{% extends 'base.html.twig' %}

{% block title %}SdtArchive{% endblock %}

{% block body %}
    <h1>SdtArchive</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sdt_archive.id }}</td>
            </tr>
            <tr>
                <th>CreateDate</th>
                <td>{{ sdt_archive.createDate ? sdt_archive.createDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Count</th>
                <td>{{ sdt_archive.count }}</td>
            </tr>
            <tr>
                <th>Acting</th>
                <td>{{ sdt_archive.acting }}</td>
            </tr>
            <tr>
                <th>ReportDate</th>
                <td>{{ sdt_archive.reportDate ? sdt_archive.reportDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sdt_archive_index') }}">back to list</a>

    <a href="{{ path('sdt_archive_edit', {'id': sdt_archive.id}) }}">edit</a>

    {{ include('sdt_archive/_delete_form.html.twig') }}
