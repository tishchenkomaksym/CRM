{% extends 'sdt/teamplate.twig' %}

{% block title %}Sdt index{% endblock %}

{% block breadcrumbs %}
    <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="{{ path('default') }}">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">Sdt list</li>
    </ol>
    </nav>{% endblock %}
{% block body %}
